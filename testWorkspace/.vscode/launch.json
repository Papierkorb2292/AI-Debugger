    "version": "0.2.0",
    "configurations": [
        {
            "type": "ai-debugger",
            "request": "launch",
            "name": "AI: Python Debugger: Current File",
            "delegate": "Python Debugger: Current File",
            "prompt": "The test in line 113 in file 'test.py' is failing, because `get_height` doesn't return the correct result"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        
    ]
}